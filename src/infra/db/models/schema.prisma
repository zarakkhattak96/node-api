// This is your Prisma schema file,
// learn more about it in the docs: https://pris.ly/d/prisma-schema
generator client {
  provider = "prisma-client-js"
}

datasource db {
  provider = "postgresql"
  url      = env("DATABASE_URL")
}

model userData {
  id       String @unique @default(uuid())
  name     String
  email    String @unique
  password String
  address  String
  phone    Int    @unique
}
